{
    "capabilities": {
        "terminal_access": true,
        "auto_install_dependencies": true,
        "ast_vector_indexing": true,
        "web_search_enabled": true,
        "self_healing_loops": {
            "enabled": true,
            "max_retry_attempts": 3
        }
    },
    "rules_of_engagement": {
        "permission_mode": "autonomous",
        "log_monitoring": [
            "star-backend/scheduler_output.txt",
            "npm_errors"
        ]
    }
}